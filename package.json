{
  "name": "balances",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "rm -rf lib && tsc",
    "db:migrate": "npx squid-typeorm-migration apply",
    "processor:start": "node lib/processor.js",
    "query-node:start": "squid-graphql-server",
    "format": "prettier --write --ignore-unknown .",
    "prepare": "husky install",
    "postinstall": "make codegen && make typegen"
  },
  "dependencies": {
    "@polkadot/util-crypto": "^10.1.4",
    "@subsquid/archive-registry": "1.0.12",
    "@subsquid/cli": "^0.6.0",
    "@subsquid/graphql-server": "^1.1.0",
    "@subsquid/ss58": "^0.1.1",
    "@subsquid/substrate-processor": "1.7.0",
    "@subsquid/typeorm-migration": "0.1.2",
    "@subsquid/typeorm-store": "0.1.2",
    "dotenv": "^10.0.0",
    "pg": "^8.7.3",
    "typeorm": "0.3.7"
  },
  "devDependencies": {
    "@subsquid/substrate-metadata-explorer": "1.0.6",
    "@subsquid/substrate-typegen": "1.2.2",
    "@subsquid/typeorm-codegen": "0.1.1",
    "@types/node": "16.11.47",
    "husky": "^8.0.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.3",
    "typescript": "4.7.4"
  }
}
